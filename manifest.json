{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Waynes New Tab",
    "version": "0.1",

    "description" : "New Tab with custom HTML and CSS",

    "author": "Wayne Hoover",

    "icons": {
        "16": "images/Waynes_NewTab_Screenshot_Corner_16x16.png",
        "32": "images/Waynes_NewTab_Screenshot_Corner_32x32.png",
        "48": "images/Waynes_NewTab_Screenshot_Corner_48x48.png",
        "128": "images/Waynes_NewTab_Screenshot_Corner_128x128.png"
    },

    "chrome_url_overrides" : {
    	"newtab": "alBlank.html"
    },

    "permissions": ["storage", "declarativeContent"],

    "options_page": "options.html",

    "background":{
        "scripts": ["background.js"],
        "persistent": false
    },

    "offline_enabled": true,
    "short_name": "NewTabCustom"
}
